#!/bin/bash

num_procs=$1

source server

echo "Summarizing queries..."
python scripts/summarize_queries.py \
       --clir_dir /clir \
       --output_dir /outputs \
       --doc_port $DOC_PORT \
       --query_port $QUERY_PORT \
       --annotation_port $ANNOTATION_PORT \
       --num_procs $num_procs \
       --queries $QUERY
